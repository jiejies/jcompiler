class Animal {
    String name;
    int age;
    
    void makeSound() {
        age = age + 1;
    }
    
    void setName(String newName) {
        name = newName;
    }
}

class Dog extends Animal {
    String breed;
    
    void bark() {
        age = age + 2;
        makeSound();
    }
    
    void setBreed(String newBreed) {
        breed = newBreed;
    }
}

class Cat extends Animal {
    boolean isIndoor;
    
    void meow() {
        age = age + 3;
        makeSound();
    }
    
    void setIndoor(boolean indoor) {
        isIndoor = indoor;
    }
} 